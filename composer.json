{
    "name": "hozana/openchurch",
    "description": "An open database for churches using Symfony 4, API platform and ElasticSearch.",
    "type": "project",
    "license": "Apache-2.0",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.1",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "fresh/doctrine-enum-bundle": "^6.2",
        "friendsofphp/php-cs-fixer": "^2.16",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "friendsofsymfony/oauth-server-bundle": "^1.6",
        "friendsofsymfony/user-bundle": "~2.0",
        "myclabs/php-enum": "^1.6",
        "phpstan/phpstan-doctrine": "^0.12.22",
        "phpstan/phpstan-symfony": "^0.12.10",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "^4.1",
        "symfony/templating": "^4.1",
        "symfony/translation": "^4.1",
        "symfony/web-server-bundle": "^4.1",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.1",
        "webonyx/graphql-php": "^14.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/browser-kit": "^4.1",
        "symfony/css-selector": "^4.1",
        "symfony/maker-bundle": "^1.9"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
